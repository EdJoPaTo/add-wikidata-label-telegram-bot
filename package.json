		"telegraf-inline-menu": "^3.5.2",
		"telegraf-session-local": "0.0.6",
		"ts-node": "^8.0.3",
		"typescript": "^3.4.1",
		"wikidata-edit": "^2.8.1",
		"wikidata-sdk": "^6.1.0",
		"wikidata-sdk-got": "^1.0.1"
	},
	"devDependencies": {
		"@types/node": "^11.12.2",
		"@typescript-eslint/eslint-plugin": "^1.3.0",
		"del-cli": "^1.1.0",
		"eslint-config-xo-typescript": "^0.9.0",
		"xo": "^0.24.0"
	},
	"xo": {
		"extends": "xo-typescript",
		"semicolon": false,
		"extensions": [
			"ts"
		],
		"rules": {
			"import/named": "warn",
			"@typescript-eslint/promise-function-async": "warn"
		}
	}
